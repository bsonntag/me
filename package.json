{
  "name": "bsonntag.me",
  "version": "0.2.1",
  "private": true,
  "description": "My personal website",
  "repository": "https://github.com/bsonntag/bsonntag.github.io",
  "license": "UNLICENSED",
  "author": "Benjamim Sonntag <benjamimsonntag@gmail.com>",
  "main": "./src/index.js",
  "scripts": {
    "bundle": "gatsby build",
    "lint": "eslint .",
    "start": "gatsby develop",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "jest-setup.js"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFilesAfterEnv": [
      "./jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.js?$": "./jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^3.6.0",
    "react-remarkbox": "^0.2.0",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6",
    "styled-tools": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@bsonntag/eslint-config": "^0.5.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.6.0",
    "gatsby": "^2.15.15",
    "gatsby-image": "^2.2.19",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sharp": "^2.2.22",
    "gatsby-plugin-styled-components": "^3.1.5",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-remark": "^2.6.22",
    "gatsby-transformer-sharp": "^2.2.14",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-dom": "^3.5.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react-testing-library": "^6.1.2",
    "sort-package-json": "^1.22.1"
  }
}
