{
  "name": "bsonntag.me",
  "version": "0.2.1",
  "private": true,
  "description": "My personal website",
  "repository": "https://github.com/bsonntag/bsonntag.github.io",
  "license": "UNLICENSED",
  "author": "Benjamim Sonntag <benjamimsonntag@gmail.com>",
  "main": "./src/index.js",
  "scripts": {
    "bundle": "gatsby build",
    "lint": "eslint .",
    "start": "gatsby develop",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "jest-setup.js"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFilesAfterEnv": [
      "./jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.js?$": "./jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.6.0",
    "react-remarkbox": "^0.2.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6",
    "styled-tools": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-seegno": "^12.0.0",
    "gatsby": "^2.1.19",
    "gatsby-image": "^2.0.30",
    "gatsby-plugin-manifest": "^2.0.20",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sharp": "^2.0.23",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.15",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.5",
    "react-testing-library": "^6.0.0",
    "sort-package-json": "^1.20.0"
  }
